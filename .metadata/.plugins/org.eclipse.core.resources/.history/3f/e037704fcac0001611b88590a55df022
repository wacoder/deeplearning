package findmedian;

public class Solution {
	public static void main(String[] args) {
		System.out.println("The result is " + 3/2);
	}
	
	public static double findMedianSortedArrays(int[] nums1, int[] nums2) {
        return 1.0;
    }
	
	public static double findKNum(int[] A, int[] B, int k) {
		int len1 = A.length;
		int len2 = B.length;
		int mid_A = null;
		int mid_B = null;
		
		if(len1 == 0) {
			return (double)B[k-1];
		}
		
		if(len2 == 0) {
			return (double)A[k-1];
		}
		
		if(k == 1) {
			return (double)Math.min(A[0], B[0]);
		}
		
		
		
		
		return 1.0;
		
		
	}
}

